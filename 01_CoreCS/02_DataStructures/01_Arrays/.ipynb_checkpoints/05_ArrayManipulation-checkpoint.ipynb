    "n, inputs = [int(n) for n in input().split(\" \")]\n",
    "list = [0]*(n+1)\n",
    "for _ in range(inputs):\n",
    "    x, y, incr = [int(n) for n in input().split(\" \")]\n",
    "    list[x-1] += incr\n",
    "    if((y)<=len(list)): list[y] -= incr;\n",
    "max = x = 0\n",
    "for i in list:\n",
    "   x=x+i;\n",
    "   if(max<x): max=x;\n",
    "print(max)"
   ]
  }
